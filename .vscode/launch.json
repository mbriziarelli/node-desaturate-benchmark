    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Jimp Benchmark",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "--jimp"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch JavaScript raw Benchmark",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "--raw"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch JavaScript threads Benchmark",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "--raw", "--threads"
            ]
        }
    ]
}